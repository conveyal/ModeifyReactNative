{
	"name": "ModeifyReactNative",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"cover": "jest --coverage --collectCoverageFrom \"js/**/*.js\"",
		"flow": "flow",
		"start": "node node_modules/react-native/local-cli/cli.js start",
		"test": "npm run flow && jest"
	},
	"dependencies": {
		"@conveyal/lonlat": "^1.3.0",
		"@conveyal/woonerf": "^2.2.1",
		"immutability-helper": "^2.1.2",
		"isomorphic-mapzen-search": "^1.2.0",
		"lodash.isequal": "^4.5.0",
		"lodash.throttle": "^4.1.1",
		"moment": "^2.17.1",
		"otp-react-redux": "https://github.com/opentripplanner/otp-react-redux.git#custom-otp-query-2",
		"polyline": "^0.2.0",
		"qs": "^6.3.1",
		"react": "~15.4.0",
		"react-native": "0.41.2",
		"react-native-gifted-form": "^0.0.13",
		"react-native-maps": "https://github.com/airbnb/react-native-maps.git",
		"react-redux": "^5.0.2",
		"redux": "^3.6.0",
		"redux-actions": "^2.0.1",
		"redux-thunk": "^2.2.0"
	},
	"devDependencies": {
		"babel-preset-react-native": "1.9.1",
		"flow-bin": "0.37.0",
		"jest": "19.0.0",
		"react-test-renderer": "~15.4.0",
		"redux-mock-store": "^1.2.2"
	},
	"jest": {
		"cacheDirectory": "tmp",
		"preset": "react-native",
		"testPathIgnorePatterns": [
			"<rootDir>/node_modules/",
			"<rootDir>/__tests__/test-utils"
		],
		"transformIgnorePatterns": [
			"node_modules/(?!apsl-react-native-button|react-native|native-base|react-clone-referenced-element|otp-react-redux)"
		]
	}
}
